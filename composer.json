{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "symfony/dom-crawler": "^3.2",
        "symfony/psr-http-message-bridge": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "cakephp/cakephp-codesniffer": "2.*",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^1.0",
        "laravel/dusk": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "2.4.*",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~5.7",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "2.6.*",
        "suitmedia/php-code-standards": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit -c tests/phpunit.xml",
        "test:all": [
            "php vendor/bin/phpunit -c tests/phpunit.xml",
            "@check-quality"
        ],
        "check-quality": [
            "vendor/bin/phpcs --colors --standard=tests/myrules.xml app/ routes/",
            "vendor/bin/phpmd app/,tests/ text codesize,controversial,naming,unusedcode --exclude app/Validator/",
            "vendor/bin/phpcpd -vn app/ --exclude app/Validator/"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
